// Vertical Timeline
.vertical-timeline.campaign-updates {
	margin: 0 auto;
	@media only screen and (max-width : @largestTabletScreen){
		padding-left: @20px;
	}
	&:before {
		width: 1px;
		background: @neutral-darkwhite;
		top: @45px;
		left: 0;
		height: auto;
		bottom: 30px;
		@media only screen and (max-width : @largestTabletScreen){
			left: auto;
		}
	}
	.vertical-timeline-element-icon {
		left: 0;
		box-shadow: none;
	}
	.vertical-timeline-element {
		&:not(:first-child) {
			margin: 80px 0 0;
			.vertical-timeline-element-icon{
				height: @25px;
				width: @25px;
				left: -@13px;
				background-color: @neutral-darkwhite !important;
				margin-left: 0;
			}
		}
		&.vertical-timeline-element--right .vertical-timeline-element-content .vertical-timeline-element-date {
			position: absolute;
			width: 78px;
			top: -60px;
			left: -220px;
			right: auto;
			padding: 0;
			background: @white;
			font-size: @13px;
			line-height: @18px;
			text-align: center;
			color: @neutral-middark;
			text-transform: uppercase;
			opacity: 1;
		}
		.ui.list .item {
			border: 1px solid @neutral-darkwhite;
		}
	}
}
.campaign-updates .vertical-timeline-element-content {
	width: 100%;
	box-shadow: none;
	padding: 0;
	background: transparent;
	left: 121px;
	@media only screen and (max-width : @largestTabletScreen){
		left: 0;
		margin-left: @30px;
		width: auto;
	}
	&::before {
		display: none;
	}
	.ui.items {
		margin-bottom: 0;
		>.item {
			font-weight: @bold;
			color: @neutral;
			.image {
				>img {
					border-radius: 50%;
					height: 60px;
					width: 60px;
				}
				&+.content {
					margin-left: @15px;
					padding: 0;
					text-transform: capitalize;
					span {
						font-size: @13px;
						font-weight: @normal;
						line-height: @20px;
					}
				}
			}
		}
		&:not(.unstackable) > .item {
			@media only screen and (max-width : @largestTabletScreen){
				flex-direction: row;
				.image+.content {
					margin-left: @14px;
				}
			}
		}
		.ui.list .item {
			border: 1px solid @neutral-darkwhite;
			>.image+.content {
				@media only screen and (max-width:@largestTabletScreen) {
					padding-left: @3px;
				}
				>.description {
					@media only screen and (max-width:@largestTabletScreen) {
						padding: 0 !important;
					}
				}
			}
		}
		p {
			font-weight: @normal;
			color: @neutral-middark;
			font-size: @13px;
			b{
				color: @neutral;
				font-size: @15px;
			}
			img {
				max-width: 100%;
				height: auto;
			}
		}
		.sb-avatar {
			width: @60px;
			height: @60px;
			line-height: @60px;
			@media only screen and (max-width : @largestTabletScreen){
				width: @40px;
				height: @40px;
				line-height: @40px;
			}
		}
	}
	.wp-content {
		max-width: 100%;
		height: auto;
		img {
			max-width: 100%;
			height: auto;
		}
	}
}


 /*******************************
        Global Overrides
*******************************/
/* PUBLIC PAGES UI STYLES ==> START */
//	iOS cursor position on keypad-in fix
html,body{
	-webkit-overflow-scrolling : touch !important;
	// overflow: auto !important;
	height: 100% !important;
}
//	iOS cursor position on keypad-in fix
body{
	color:@neutral-middark;
}
.public-pages {
	background: @white;
	position: relative;
	max-width: 1680px;
	min-height: 100vh;
	margin: 0 auto;
	box-shadow: 0px 4px 10px @thumbInactiveBackground;
	// removed because it was creating issue in data room section - moving UI to left
	// &:before{
	// 	content: '';
	// 	position: absolute;
	// 	left: -100%;
	// 	right: -100%;
	// 	top: 0;
	// 	bottom: 0;
	// 	background-color: @white;
	// 	z-index: -1;
	// }
	a:not(.item), a:not(.ui.button) {
		cursor: pointer !important;
		&:hover, &:active, &:focus {
			text-decoration: none !important;
		}
	}
}
.clearfix::after{
	content: "";
	clear: both;
	display: table;
}
section{
	padding:@50px 0;
	@media only screen and (max-width : @largestMobileScreen){
		padding:@30px 0;
	}
	.caption{
		margin-bottom:@50px !important;
	}
	&.padded{
		padding-left: @50px;
		padding-right: @50px;
	}
}
//	Helper Classes Starts
iframe{
	border:none;
}
//	Heading font sizes
h1, h1.ui.header{
	font-weight:@normal;
	line-height:66px;
	@media only screen and (max-width : @largestMobileScreen){
		font-size: @28px !important;
		line-height: 38px;
	}
}
h2, h2.ui.header{
	// line-height:34px;
	@media only screen and (max-width : @largestMobileScreen){
		font-size: @28px !important;
		line-height: 38px;
	}
}
h3, h3.ui.header{
	@media only screen and (max-width : @largestMobileScreen){
		font-size: @20px !important;
		line-height: 24px;
	}
}
h4, h4.ui.header{
	line-height:24px;
}
h5, h5.ui.header{
	font-weight:@bold;
}
h6{
	font-size:@14px;
	font-weight:@fontMedium;
}
*:focus{
	outline:none;
}
.mb-0{
	margin-bottom:0 !important;
}
.mb-10 {
	margin-bottom: @10px !important;
}
.mb-20 {
	margin-bottom: @20px !important;
}
.mb-30 {
	margin-bottom: @30px !important;
}
.mb-40 {
	margin-bottom: @40px !important;
}
.mb-50 {
	margin-bottom: @50px !important;
}
.mb-60{
	margin-bottom:@60px !important;
}
.mb-80 {
	margin-bottom: @80px !important;
}
.mt-0 {
	margin-top: 0 !important;
}
.mt-10 {
	margin-top: @10px !important;
}
.mt-20 {
	margin-top: @20px !important;
}
.mt-30 {
	margin-top: @30px !important;
}
.mt-40 {
	margin-top: @40px !important;
}
.mt-50 {
	margin-top: @50px !important;
}
.mt-74 {
	margin-top: @74px;
}
.mt-80 {
	margin-top: @80px !important;
}
.mb-half{
	margin-bottom:@7px !important;
}
.mt-half{
	margin-top:@7px !important;
}
.mr-10 {
	margin-right: @10px !important;
}
.ml-10 {
	margin-left: @10px !important;
}
.pl-50{
	padding-left: @50px !important;
}
.pull-left{
	float:left !important;
}
.pull-right{
	float:right !important;
}
.left-align{
	text-align:left !important;
}
.right-align{
	text-align:right !important;
}
.center-align{
	text-align:center !important;
	justify-content:center;
}
.justify-text {
	text-align: justify;
}
.highlight-text{
	color:@primary-one !important;
}
.primary-text {
	color: @primaryColor;
}
.secondary-text {
	color: @secondary-color;
}
.text-uppercase {
	text-transform: uppercase !important;
}
.text-capitalize {
	text-transform:capitalize !important;
}
.inverse-text{
	color:@white !important;
}
.positive-text{
	color:@primary-one !important;
}
.negative-text{
	color:@highlight-primary !important;
}
.tertiary-text{
	color:@tertiary-text !important;
}
.warning-text{
  color:@highlight-secondary !important;
}
.intro-text, .note{
	color:@neutral-middark !important;
	font-size: @12px;
}
.primary-two-text{
	color: @primary-two !important;
}
.neutral-text {
	color: @neutral !important;
}
.grey-text {
	color: @border-color;
}
.grey-header {
	color: @neutral-middark !important;
}
.caption-note{
	font-size:@12px;
	font-style:italic;
	margin:@7px 0;
	&:first-child{
		margin-top:0px;
	}
	&:last-child{
		margin-bottom:0px;
	}
}
.regular-text {
	font-weight: @normal !important;
}
.more-info {
	font-size: 12px;
}
//	normal link styles
a:not(.item), a:not(.ui.button){
	color:@link-color;
	&:hover{
		text-decoration:underline !important;
	}
}
.info-card{
	.ui.button>.icon:not(.button){
		line-height: 1;
		vertical-align: middle;
		height: auto;
	}
	img.ui.image {
		height: 52px;
	}
	&.home-summary {
		margin: 0 @35px;
		.ui.button {
			width: 150px;
		}
	}
	&.campaign img.ui.image {
		height: auto !important;
	}
}
a{
	&.inverted{
		color:@white;
	}
	&.secondary-link{
		color:@primary-two !important;
	}
	&.text-link{
		color:@neutral;
		&:hover, &:active, &.active{
			color:@neutral;
		}
	}
	&.grey-link{
		font-size:@13px;
		color:@neutral-offdark;
		text-decoration:none !important;
		&:hover, &:active, &.active{
			color:@neutral-middark;
		}
	}
	&.green-hover:hover{
		color: @primary-one !important;
	}
}
.green-hover.ui.button.link-button.grey:hover {
	color: @primary-one !important;
}
.invisible{
	opacity:0 !important;
}
.bg-offwhite {
	background-color: @neutral-offwhite;
	// padding:@14px @20px;       commented to resolve on public homepage
}
.green-block {
	background-color: @primary-one;
}
.blue-block {
	background-color: @secondary-color;
}
.primary-block {
	background-color: @primary-two;
}
i.icon.right {
	margin-left: 0.25rem;
}
.no-pointer-events {
	pointer-events: none !important;
}
.parsed-data { // for migrated/parsed data
	.fr-view blockquote {
		border-left: solid 3px @primary-one;
		padding: @15px @20px;
		color: @neutral;
		font-size: @16px;
		font-style: italic;
		background: @neutral-offwhite;
	}
	.wp-caption {
		max-width: 100%;
		height: auto;
	}
	img {
		max-width: 100%;
		height: auto;
	}
}
//	Helper Classes Ends

// Footer Styles
footer{
	background-color:@neutral-offwhite;
	padding: @50px 0;
	max-width: 1680px;
    margin: auto;
	.footer-left {
		display: flex !important;
		flex-direction: column !important;
		.footer-left-nav {
			flex-grow: 1;
			display: flex;
			flex-direction:column;
			justify-content: space-between;
			@media only screen and (max-width : @largestMobileScreen){
				flex-direction: column;
				align-items: center;
			}
			@media only screen and (min-width : @tabletBreakpoint) and (max-width : @largestTabletScreen){
				justify-content: center;
			}
		}
	}
	.footer-social {
		flex-grow: 0;
		@media only screen and (max-width : @largestMobileScreen){
			text-align: center;
		}
		@media only screen and (min-width : @tabletBreakpoint) and (max-width : @largestTabletScreen){
			text-align: center;
			margin-bottom: @20px;
		}
		a.item {
			display: inline-block;
			color: @neutral-middark;
			&:hover {
				text-decoration: none;
			}
		}
		p {
			display: inline-block;
			margin-left: 60px;
			@media only screen and (max-width : @largestMobileScreen){
				display: block;
				margin-left: 0;
			}
		}
	}
	@media only screen and (max-width : @largestMobileScreen){
		.secure img{
			margin: auto;
		}
	}
	.copyright-info{
		flex-grow: 1;
		font-size:@13px;
		line-height:@22px;
		@media only screen and (max-width : @largestMobileScreen){
			float:none;
		}
		a {
			color: @neutral-middark !important;
			text-decoration: underline !important;
			&:hover, &:focus{
				text-decoration: underline !important;
			}
		}
	}
	&.offering-footer .footer-social p{
		display: block;
		margin-top: @14px;
		margin-left: 0;
	}
}
.public-pusher footer {
	@media only screen and (max-width : @computerBreakpoint){
		padding-bottom: @42px;
	}
}
/** campaign filter **/
.campaign-list-wrapper {
	position: relative;
	h5.ui.header {
		color: @neutral-middark;
		margin-bottom: @35px;
	}
}
.offer-filter {
	position: absolute;
    height: 100%;
    width: 100%;
    left: 0;
	top: ~'calc(100% + 1px)';
	z-index: 1;
	.offer-filter-container {
		background-color: @white;
		width: 100%;
		padding: @17px 0 @50px;
		h6.ui.header {
			font-size: @13px;
			font-weight: @bold;
			color:@neutral-middark;
			padding-bottom: @6px;
		}
		.ui.checkbox {
			label {
				color:@neutral-middark;
			}
		}
		i {
			font-size: @16px;
			line-height: @16px;
			color:@neutral-darkwhite;
			margin-top: @3px;
		}
		.checkbox-group {
			.ui.label {
				background-color: transparent;
				color: @neutral-middark;
				padding: 0;
				padding-left: @3px;
				font-weight: normal;
			}
		}
		.slick-slider {
			padding-bottom: @30px;
		}
		.ui.buttons {
			position: absolute;
			bottom: 60px;
		}
		.filter-buttons .ui.buttons {
			display: block;
			position: static;
			.ui.button {
				height: 14px;
				font-size: @14px;
				line-height: 14px;
				>.icon {
					vertical-align: top;
					font-size: @14px;
					line-height: 14px;
    				opacity: 1;
					height: 14px;
				}
			}
		}
	}
}
.overlay{
	position:absolute;
	top:0;
	right:0;
	bottom:0;
	left:0;
	background-color:fade(@neutral, 30%);
}

/** Campaign Details **/
.campaign-details-banner{
	.raise-type{
		font-size: @18px;
		color: @white;
		line-height: 28px;
		i.icon{
			font-size: @14px;
		}
	}
	small{
		display:inline-block;
		margin-top:@7px;
		font-size:@13px;
		color:@white;
	}
}
// .offer-details {
// 	min-height:~"calc(100vh - 58px)";
// }
.offering-side-menu {
	@media only screen and (max-width : @largestTabletScreen){
		background:url(../../assets/images/banners/campaign-details.png) top center no-repeat / cover;
		background-color: @neutral;
		background-blend-mode:multiply;
		position: static;
		overflow: hidden;
		width: 100%;
		max-height: 1000px;
		height: auto;
		transition: all 500ms ease-in-out;
		padding: 0;
		z-index: 1;
		&.collapse {
			max-height: 0;
		}
	}
	.ui.menu, .ui.vertical.menu {
		border: none;
		box-shadow: none;
	}
	.progress-bar {
		height: 210px;
		width: 210px;
	}
	p {
		font-size: @13px;
		color: @white;
		text-align: center;
		margin: @14px 0;
	}
}
.offering-intro {
	padding: @20px @20px 0;
	@media only screen and (max-width : @largestTabletScreen){
		padding: @20px @32px;
	}
	h4.ui.header {
		position: relative;
		text-overflow: ellipsis;
		white-space: nowrap;
		width: 100%;
		overflow: hidden;
		> a {
			position: absolute;
			left: 0;
			font-size: @12px;
			color:@neutral-middark;
		}
	}
}
.no-early-bird {
	margin: @30px auto;
}
.campaign-content-wrapper {
	// padding: @40px;
	// flex-grow: 1;
	// // background-color: @white;
	// @media only screen and (max-width : @largestMobileScreen){
	// 	padding: @40px 0;
	// }
	p {
		color: @neutral-middark;
		font-size: @15px;
		&.note {
			color:@neutral-middark;
			font-size: @12px;
		}
		&.early-bird-desc {
			line-height: @24px;
		}
		i.icon {
			margin-left:@7px;
		}
		img {
			max-width: 100%;
		}
	}
	.embed img{
		margin-top: 0;
	}
	.history-section.ui.list >.item:not(:last-child){
		margin-bottom: @30px;
		padding-bottom: 0;
	}
	.pdf-viewer {
		display: flex;
		flex-direction: column;
		min-height:70vh;
		> object, >iframe {
			flex: 1;
		}
	}
	.ui.padded.segment h6.ui.header {
		margin: @7px 0;
	}
}

/** Share icons **/
.share-icons {
	margin-bottom: 30px;
    display: flex;
    flex-direction: row;
    a {
        display: flex;
        flex-direction: column;
        margin:0 10px;
		color:@neutral-middark;
		&:hover, &:active, &:focus{
			text-decoration:none;
		}
    }
	i.icon.circular.inverted{
		color:@white;
		background-color:@neutral-darkwhite !important;
		margin:0 0 @10px;
		&.default {
			color:inherit !important;
		}
		&.fb-color {
			background-color:@facebookColor !important;
			&:hover {
				background-color:@facebookHoverColor !important;
			}
		}
		&.tw-color {
			background-color:@twitterColor !important;
			&:hover {
				background-color:@twitterHoverColor !important;
			}
		}
		&.in-color {
			background-color:@linkedInColor !important;
			&:hover {
                background-color:@linkedInHoverColor !important;
            }
        }
    }
}


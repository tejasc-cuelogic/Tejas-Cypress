.ui.grid.investment-scale{
	margin-bottom:@40px;
	.invested{
		margin:0 ~"calc(100% / 7 / 2)";
		padding:0 !important;
		width:100%;
		min-height:40px;
		position:relative;
		.investment-progress{
			content:"";
			height:3px;
			background:lighten(@link-color, 20%);
			display:block;
			position:absolute;
			bottom:-41px;
			z-index:1;
		}
		>.amount{
			min-width:calc(100% / 7);
			padding:0 1rem;
			position:absolute;
			top:15px;
			left:0;
			z-index:3;
			transform:translate(-50%, 0%);
			font-size:@13px;
			font-weight:700;
			color:@neutral-middark;
			>span{
				font-size:@14px;
				color:@link-color;
				display:block;
			}
			&:after{
				content:"";
				width:13px;
				height:13px;
				border-radius:@8px;
				background-color:@link-color;
				position:absolute;
				left:50%;
				bottom:-40px;
				transform:translate(-50%, -50%);
				border:3px solid white;
				box-shadow:0px 0px 4px 1px @link-color;
			}
		}
	}
	.column{
		position:relative;
		padding-top:@50px;
		font-weight:700;
		color:@border-color;
		&:before{
			content:"";
			height:1px;
			width:100%;
			position:absolute;
			left:50%;
			top:25px;
			background-color:@border-color;
		}
		&:after{
			content:"";
			width:9px;
			height:9px;
			border-radius:@5px;
			background-color:@neutral-middark;
			position:absolute;
			left:50%;
			top:25px;
			z-index:2;
			transform:translate(-50%, -50%);
			border:2px solid white;
			box-shadow:0px 0px 4px 1px @neutral-middark;
		}
		&:last-child:before{
			display:none;
		}
		&.crossed{
			color:@link-color;
			&:after{
				background-color:lighten(@link-color, 20%);
				box-shadow:0px 0px 4px 1px @link-color;
			}
		}
	}
}


.highlight-text, .negative-text {
	&.large {
		font-size: @20px;
		font-weight: @fontMedium;
		vertical-align: middle;
	}
}

//	Add-withdraw fund popup
.fund-amount h4.ui.header {
	font-size: @24px;
	span {
		font-size: @12px;
		cursor: pointer;
		margin-left: @10px;
	}
}

//	Team Member Details Modal START
.team-avatar-wrap{
	position:relative;
	display:inline-block;
	margin-left:@30px;
	.ui.button{
		position:absolute;
		right:-5px;
		bottom:-5px;
	}
	.ui.image.avatar {
		height: 60px;
		width: 60px;
		line-height:28px;
	}
}
//	Team Member Details Modal ENDS

//	Campaign Re-design
.video-wrapper {
	position:relative;
	display: block;
	&:hover{
		text-decoration: none;
	}
	a {
		position: relative;
		display:block;
		&:hover {
			text-decoration: none;
		}
	}
	.play-icon{
		font-size: 80px;
		line-height: 80px;
		color: @white;
		position:absolute;
		top:50%;
		left:50%;
		transform:translate(-50%,-50%);
		z-index: 1;
		cursor: pointer;
	}
	img.ui.image {
		height: auto;
		width: 100%;
	}
	~.social-links a{
		margin-right:@7px;
		color: @white;
		&:hover {
			text-decoration: none;
		}
		i.icon{
			vertical-align: middle;
			line-height: 1;
			margin:0;
		}
	}
	&.campaign {
		padding-top: 56.25%;
		a, [imgtype="heroImage"]{
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			background-size: cover;
			background-repeat: no-repeat;
			background-position: center center;
		}
	}
}

.anchor-wrap{
	position:relative;
	>.anchor, >.anchor-scroll{
		height:1px;
		width:1px;
		position:absolute;
		left:0;
		top:-130px;
	}
	>.anchor-scroll{
		top:-60px;
	}
}
.recharts-tooltip-wrapper {
	background-color: @white;
	padding: @14px;
	font-weight: @bold;
	position: relative;
	box-shadow: @commonShadow;
	&.recharts-tooltip-wrapper-right::before {
		content: '';
		position: absolute;
		left: -14px;
		top: 50%;
		transform: translate(0, -50%);
		width: 0;
		height: 0;
		border-style: solid;
		border-width: 15px 15px 15px 0;
		border-color: transparent @white transparent transparent;
	}
	&.recharts-tooltip-wrapper-left::after {
		content: '';
		height: 15px;
		width: 15px;
		position: absolute;
		right: -14px;
		top: 50%;
		transform: translate(0, -50%);
		border-style: solid;
		border-width: 15px 0 15px 15px;
		border-color: transparent transparent transparent @white;
	}
}
.fr-view{
	span[style~="color:"] a {
		color: @primary-one !important;
	}
	table{
		th, td{
			border:none !important;
		}
	}
	.video-container iframe, iframe{
		height: 400px;
		width: 100%;
		@media only screen and (max-width : @largestMobileScreen){
			height: auto;
		}
	}
}
.field .pac-container {
	left: 0 !important;
	top: 63px !important;
}
.plaid-link-iframe {
	@media only screen and (max-width : @largestMobileScreen){
		position: absolute;
		z-index: 9999;
	}
}
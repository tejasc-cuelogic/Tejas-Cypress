//	Multistep modal navigation
ol.progtrckr{
	margin:0;
	padding:@24px 0 @26px;
	list-style-type:none;
	text-align:center;
	li{
		min-width:60px;
		max-width:100px;
		margin:0 @7px @14px;
		padding-bottom:@14px;
		display:inline-block;
		font-size:@12px;
		font-weight:@fontMedium;
		line-height:@16px;
		text-align:center;
		cursor:pointer;
		position:relative;
		&:after{
			content:"";
			width:60px;
			height:4px;
			border-radius:@4px;
			display:block;
			background:@neutral-offdark;
			position:absolute;
			left:50%;
			bottom:0;
			transform:translate(-50%, 0%);
		}
		&.hidden{
			display:none !important;
		}
		&.progtrckr-todo{
			color:@neutral-middark;
			&:after{
				background:@neutral-middark;
			}
		}
		&.progtrckr-doing{
			color:@primary-one;
			pointer-events: none;
			&:after{
				background:@primary-one;
			}
		}
		&.progtrckr-done{
			color:@primary-one;
			opacity: 0.45;
			&:after{
				background:@primary-one;
			}
		}
		&.progtrckr-todo, &.progtrckr-doing, &.progtrckr-done{
			&.error{
				color:@highlight-primary;
				&:after{
					background:@highlight-primary;
				}
			}
		}
	}
}

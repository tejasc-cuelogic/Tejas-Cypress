		&.accredited:after{
			background-color:@highlight-tertiary;
		}
	}
	&.partial{
		color:@highlight-secondary;
		border-color:@highlight-secondary;
		&.accredited:after{
			background-color:@highlight-secondary;
		}
	}
	&.locked{
		color:@highlight-primary;
		border-color:@highlight-primary;
		&.accredited:after{
			background-color:@highlight-primary;
		}
	}
	&.small{
		width:24px;
		height:24px;
		font-size:@15px;
		border-width:1px;
		&.accredited:after{
			font-size:@8px;
			width:13px;
			height:13px;
			top:-6px;
			right:-6px;
		}
	}
	&.medium{
		width:36px;
		height:36px;
		font-size:@22px;
		border-width:2px;
		&.accredited:after{
			font-size:@11px;
			width:16px;
			height:16px;
			top:-8px;
			right:-8px;
		}
	}
}

